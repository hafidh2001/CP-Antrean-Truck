<?php

class MCharges extends ActiveRecord
{

	public function tableName()
	{
		return 'm_charges';
	}

	public function rules()
	{
		return array(
			array('name', 'required'),
			array('name', 'length', 'max'=>256),
		);
	}

	public function relations()
	{
		return array(
			'tOutletCharges' => array(self::HAS_MANY, 'TOutletCharges', 'id_charges'),
		);
	}

	public function attributeLabels()
	{
		return array(
			'id' => 'ID',
			'name' => 'Name',
		);
	}

}
