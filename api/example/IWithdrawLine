<?php

class IWithdrawLine extends ActiveRecord
{

	public function tableName()
	{
		return 'i_withdraw_line';
	}

	public function rules()
	{
		return array(
			array('id_withdraw, sales_order_number, amount', 'required'),
			array('id_withdraw', 'numerical', 'integerOnly'=>true),
			array('sales_order_number', 'length', 'max'=>256),
		);
	}

	public function relations()
	{
		return array(
			'idWithdraw' => array(self::BELONGS_TO, 'IWithdraw', 'id_withdraw'),
		);
	}

	public function attributeLabels()
	{
		return array(
			'id' => 'ID',
			'id_withdraw' => 'Id Withdraw',
			'sales_order_number' => 'Sales Order Number',
			'amount' => 'Amount',
		);
	}

}
